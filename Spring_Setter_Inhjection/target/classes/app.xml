<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="address1" class="com.ibm.Address">
		<property name="addressLine1" value="Tapovanam"></property>
		<property name="city" value="Anantapur"></property>
		<property name="state" value="Andhra Pradesh"></property>
		<property name="country" value="India"></property>
	</bean>

	<bean id="emp" class="com.ibm.Employee">
		<property name="id" value="1"></property>
		<property name="name" value="Rewanth Sai Simha"></property>
		<property name="salary" value="20000"></property>
		<property name="address" ref="address1"></property>
	</bean>

</beans>
